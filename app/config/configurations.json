{
	"site":{
		"default_language": "en"
	},
	"models": {

		"user": {
			"api": {
				"get": {
					"all": "/user",
					"one": "/user/:id"
				},
				"post": "/user",
				"patch": "/user/:id",
				"delete": "/user/:id"
			},
			"relationships": {
				"groups": {
					"type": "group",
					"isArray": true,
					"cascadeDelete": false
				},
				"preference": {
					"type": "user_preference",
					"isArray": false,
					"cascadeDelete": false
				}
			},
			"singular": "user",
			"plural": "users",
			"structure": {
				"first_name": {
					"type": "text",
					"mode": ["list", "show", "create", "edit"]
				},
				"last_name": {
					"type": "text",
					"mode": ["list", "show", "create", "edit"]
				},
				"username": {
					"type": "text",
					"mode": ["list", "show", "create", "edit"]
				},
				"email": {
					"type": "text",
					"mode": ["list", "show", "create", "edit"]
				}
			}
		},
		"user_preference":{
			"api": {
				"get": {
					"all": "/user_preference",
					"one": "/user_preference/:id"
				},
				"post": "/user_preference",
				"patch": "/user_preference/:id",
				"delete": "/user_preference/:id"
			},
			"relationships": {
				"user": {
					"type": "user",
					"isArray": false,
					"cascadeDelete": true
				}
			},
			"singular": "preference",
			"plural": "preferences",
			"structure": {
				"language": {
					"type":"text",
					"mode": ["list", "show", "create", "edit"]
				},
				"admin_skin": {
					"type": "text",
					"mode": ["list", "show", "create", "edit"]
				}
			}
		},
		"group": {
			"api": {
				"get": {
					"all": "/group",
					"one": "/group/:id"
				},
				"post": "/group",
				"patch": "/group/:id",
				"delete": "/group/:id"
			},
			"relationships": {
				"users": {
					"type": "user",
					"isArray": true,
					"cascadeDelete": false
				}
			},
			"singular": "group",
			"plural": "groups",
			"structure": {
				"name": {
					"type": "text",
					"mode": ["list", "show", "create", "edit"]
				},
				"weight": {
					"type": "number",
					"mode": ["list", "show", "create", "edit"]
				}
			}
		},
		"machine_name": {
			"api": {
				"get": {
					"all": "/machine_name",
					"one": "/machine_name/:id"
				},
				"post": "/machine_name",
				"patch": "/machine_name/:id",
				"delete": "/machine_name/:id"
			},
			"relationships": {
				"translations": {
					"type": "machine_name_translation",
					"isArray": true,
					"cascadeDelete": false
				}
			},
			"singular": "machineName",
			"plural": "machineNames",
			"structure": {
				"name": {
					"type": "text",
					"mode": ["list", "show", "create", "edit"]
				}
			}
		},
		"machine_name_translation": {
			"api": {
				"get": {
					"all": "/machine_name_translation",
					"one": "/machine_name_translation/:id"
				},
				"post": "/machine_name_translation",
				"patch": "/machine_name_translation/:id",
				"delete": "/machine_name_translation/:id"
			},
			"relationships": {
				"machineName": {
					"type": "machine_name",
					"isArray": false,
					"cascadeDelete": false
				}
			},
			"singular": "machineNameTranslation",
			"plural": "machineNameTranslations",
			"structure": {
				"translation": {
					"type": "text",
					"mode": ["list", "show", "create", "edit"]
				},
				"language": {
					"type": "select",
					"mode": ["list", "show", "create", "edit"]
				}
			}
		}
	},
	"providers": {
		"ResourceManager": {
			"storage": "ResourceStorage",
			"rest": "JsonApiRest",
			"converter": "JsonApiResourceConverter"
		}
	},
	"url": {
		"api": "http://localhost:3000/api"
	}
}